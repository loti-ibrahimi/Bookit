{{#each booklists}}
  <section class="ui segment">
    <h2 class="ui header">
      {{title}}
    </h2>
    <hr>
    <p> Total Sources: {{sources}} </p>
    <a href="/booklist/{{id}}" class="ui icon button">
      <i class="icon folder open"></i>
    </a>
    <a href="/bookmarks/deletebooklist/{{id}}" class="ui icon button">
      <i class="icon trash"></i>
    </a>
  </section>
{{/each}}